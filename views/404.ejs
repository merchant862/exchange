<%- include('./partials/header.ejs') %>

<% if(typeof status != undefined) { %>

  <% if(status=="loggedIn") { %>
    
    <%- include('./partials/authenticated-menu.ejs') %>

  <% } else {%>
   
    <%- include('./partials/unauthenticated-menu.ejs') %>

  <% } %>

<% } %>

  <div class="error-page vh-100 d-flex justify-content-center text-center">
    <div class="my-auto">
      <h2>404</h2>
      <p>Oops something went wrong</p>

      <% if(typeof status != undefined) { %>

        <% if(status=="loggedIn") { %>
          
          <a href="/" class="btn">Back to Home <i class="icon ion-md-home"></i></a>
      
        <% } else {%>
         
          <a href="/login" class="btn">Back to Login <i class="icon ion-md-home"></i></a>
      
        <% } %>
      
      <% } %>
      
       
    
    </div>
  </div>
  
  <%- include('./partials/scripts.ejs') %>
  
</body>

</html>